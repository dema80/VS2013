<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="TherapeuticAPI" namespace="TherapeuticAPI.Models" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Users" table="users" lazy="true" >
    <id name="Login" column="login" />
    <property name="Pwd">
      <column name="pwd" sql-type="character varying" not-null="false" />
    </property>
  </class>
</hibernate-mapping>